{
  "name": "react-powertimeline",
  "version": "0.1.1",
  "description": "High-performance, interactive React component for visualizing multi-layered time-series data",
  "type": "module",
  "main": "./dist/powertimeline.umd.js",
  "module": "./dist/powertimeline.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/powertimeline.es.js",
      "require": "./dist/powertimeline.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:types": "tsc",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:visual": "playwright test",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "examples": "node scripts/examples-server.js",
    "examples:basic": "npm run examples basic-timeline",
    "examples:large": "npm run examples large-dataset",
    "examples:accessibility": "npm run examples accessibility-demo",
    "examples:styling": "npm run examples custom-styling",
    "examples:interactive": "npm run examples interactive-features",
    "release:check": "npm run lint && npm test -- --run && npm run build",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "timeline",
    "visualization",
    "d3",
    "typescript",
    "component-library",
    "time-series",
    "data-visualization"
  ],
  "author": "PowerTimeline Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-powertimeline.git"
  },
  "bugs": {
    "url": "https://github.com/react-powertimeline/issues"
  },
  "homepage": "https://github.com/react-powertimeline#readme",
  "peerDependencies": {
    "react": ">=18.2.0",
    "react-dom": ">=18.2.0"
  },
  "dependencies": {
    "d3-axis": "^3.0.0",
    "d3-scale": "^4.0.2",
    "d3-selection": "^3.0.0",
    "d3-shape": "^3.2.0",
    "d3-time": "^3.1.0",
    "d3-time-format": "^4.1.0",
    "d3-zoom": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.13",
    "@storybook/addon-links": "^9.1.13",
    "@storybook/addon-onboarding": "^9.1.13",
    "@storybook/addon-storysource": "^8.6.14",
    "@storybook/react-vite": "^9.1.13",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/d3-axis": "^3.0.6",
    "@types/d3-scale": "^4.0.8",
    "@types/d3-selection": "^3.0.11",
    "@types/d3-shape": "^3.1.6",
    "@types/d3-time": "^3.0.3",
    "@types/d3-time-format": "^4.0.3",
    "@types/d3-zoom": "^3.0.8",
    "@types/minimatch": "^6.0.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "eslint-plugin-storybook": "^9.1.13",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^15.0.2",
    "playwright": "^1.40.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^9.1.13",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
